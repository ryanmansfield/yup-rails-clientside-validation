<div id="<%= dom_id todo %>">
  <p>
    <strong>Name:</strong>
    <%= todo.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= todo.description %>
  </p>

  <p>
    <strong>Due date:</strong>
    <%= todo.due_date %>
  </p>

  <p>
    <strong>Review required:</strong>
    <%= todo.review_required %>
  </p>

  <p>
    <strong>Priority:</strong>
    <%= todo.priority %>
  </p>

</div>
